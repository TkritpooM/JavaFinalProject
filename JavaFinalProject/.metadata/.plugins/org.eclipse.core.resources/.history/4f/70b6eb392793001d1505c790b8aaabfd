package Instance;

import java.awt.Font;

import javax.swing.JRadioButton;

public class CreateRadioButton {
	
	public static JRadioButton CreateRadioButton(String RadioName,int x,int y,int w,int h,int font) {
		JRadioButton blank = new JRadioButton("Dunk low panda");
		blank.setFont(new Font("Mali-Bold", Font.BOLD, 18));
		blank.setFocusable(false);
		blank.setBounds(160, 250, 300, 50);
		return blank;
	}

}
